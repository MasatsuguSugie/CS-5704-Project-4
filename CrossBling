package ducksim;

/**
 * <p>
 * This class puts a "+" String on the duck.
 * </p>
 * 
 * @author sugie
 * @version Apr 25, 2019
 */
public class CrossBling
    extends Bling
{

    /**
     * <p>
     * Create a new CrossBling object.
     * </p>
     * 
     * @param d
     */
    public CrossBling(Duck d)
    {
        super(d);
    }


    @Override
    public String display()
    {
        return duck.display() + ":+";
    }

}
