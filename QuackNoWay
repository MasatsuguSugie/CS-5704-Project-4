package ducksim;

/**
 * <p>
 * This is the quack no way class. It sets the ducks quacking behavior as swimming.
 * </p>
 * 
 * @author sugie
 * @version Apr 24, 2019
 */
public class QuackNoWay
    implements QuackBehavior
{

    /**
     * <p>
     * {@inheritDoc}
     * </p>
     */
    @Override
    public State getState()
    {
        return State.SWIMMING;
    }


    /**
     * <p>
     * {@inheritDoc}
     * </p>
     */
    @Override
    public String getQuack()
    {
        return "";
    }
}
