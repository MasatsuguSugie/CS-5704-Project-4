package ducksim;

/**
 * <p>
 * This class puts a ")" String on the duck.
 * </p>
 * 
 * @author sugie
 * @version Apr 25, 2019
 */
public class MoonBling
    extends Bling
{

    /**
     * <p>
     * Create a new MoonBling object.
     * </p>
     * 
     * @param d
     */
    public MoonBling(Duck d)
    {
        super(d);
    }


    @Override
    public String display()
    {
        return duck.display() + ":)";
    }

}
